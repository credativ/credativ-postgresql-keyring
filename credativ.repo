[credativ-arch]
name=Elephant Shed for Red Hat/CentOS $releasever - $basearch
baseurl=https://packages.credativ.com/public/postgresql/yum/el$releasever/$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/CREDATIV-KEY

[credativ-noarch]
name=Elephant Shed for Red Hat/CentOS $releasever - noarch
baseurl=https://packages.credativ.com/public/postgresql/yum/el$releasever/noarch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/CREDATIV-KEY
